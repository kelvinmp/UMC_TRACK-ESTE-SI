{
  "expo": {
    "name": "my-proyect",
    "entryPoint": "node_modules/expo-router/entry",
    "scheme": "UMC_TRACK",
    "slug": "my-proyect",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/maritima.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/maritima.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": {
            "Montserrat-ExtraLight": "./assets/fonts/Montserrat-ExtraLight.ttf",
            "Montserrat-Bold": "./assets/fonts/Montserrat-Bold.ttf",
            "Montserrat-Regular": "./assets/fonts/Montserrat-Regular.ttf"
          }
        }
      ]
    ]
  }
}
